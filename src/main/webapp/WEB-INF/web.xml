   </servlet-mapping>
   
   <servlet>
    <description></description>
    <display-name>RegisterBatchController</display-name>
    <servlet-name>RegisterBatchController</servlet-name>
    <servlet-class>com.sofi.controller.RegisterBatchController</servlet-class>
   
   </servlet>
   
   <servlet-mapping>
    <servlet-name>RegisterBatchController</servlet-name>
    <url-pattern>/RegisterBatchController</url-pattern>
    <url-pattern>/RegisterBatch</url-pattern>
   
   </servlet-mapping>
   
    
