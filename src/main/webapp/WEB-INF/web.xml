    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
    <servlet>
    <description></description>
    <display-name>WelcomeAdminController</display-name>
    <servlet-name>WelcomeAdminController</servlet-name>
    <servlet-class>com.sofi.controller.WelcomeAdminController</servlet-class>
   </servlet>
  <servlet-mapping>
    <servlet-name>WelcomeAdminController</servlet-name>
    <url-pattern>/WelcomeAdminController</url-pattern>
    <url-pattern>/WelcomeAdmin</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <description></description>
    <display-name>AddParticipantController</display-name>
    <servlet-name>AddParticipantController</servlet-name>
    <servlet-class>com.sofi.controller.AddParticipantController</servlet-class>
   </servlet>
  <servlet-mapping>
    <servlet-name>AddParticipantController</servlet-name>
    <url-pattern>/AddParticipantController</url-pattern>
    <url-pattern>/AddParticipant</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <description></description>
    <display-name>LogoutServlet</display-name>
